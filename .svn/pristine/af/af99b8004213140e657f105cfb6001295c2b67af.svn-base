<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical">

    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="630px"
        android:background="@color/white_f1">

        <ImageView
            android:id="@+id/stu_info_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            android:src="@drawable/stu_info_back" />

        <android.support.v7.widget.Toolbar xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/transparent"
            android:elevation="1dp"
            android:minHeight="?attr/actionBarSize">

            <TextView
                android:id="@+id/tv_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:singleLine="true"
                android:textColor="#ffffff"
                android:textSize="20sp" />
        </android.support.v7.widget.Toolbar>

        <ImageView
            android:id="@+id/iv_line"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="340px"
            android:background="@android:color/white" />

        <ImageView
            android:id="@+id/iv_head"
            android:layout_width="200px"
            android:layout_height="240px"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="170px"
            android:layout_marginLeft="50px"
            android:src="@mipmap/test_head" />

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/iv_line"
            android:layout_marginBottom="30px"
            android:layout_marginLeft="60px"
            android:layout_toEndOf="@id/iv_head"
            android:layout_toRightOf="@id/iv_head">

            <TextView
                android:id="@+id/tv_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="小明"
                android:textColor="@android:color/white"
                android:textSize="50px" />

            <TextView
                android:id="@+id/tv_sex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40px"
                tools:text="女"
                android:textColor="@android:color/white"
                android:textSize="50px" />
        </com.zhy.autolayout.AutoLinearLayout>

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignTop="@id/iv_line"
            android:layout_marginTop="10sp"
            android:layout_toEndOf="@id/iv_head"
            android:layout_toRightOf="@id/iv_head"
            android:orientation="horizontal">


            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="250px"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40px"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="right|center_vertical"
                    android:text="证件号码："
                    android:textColor="@android:color/white"
                    android:textSize="35px" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20px"
                    android:text="@string/birthday"
                    android:textColor="@android:color/white"
                    android:textSize="35px" />

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20px"
                    android:text="名族："
                    android:textColor="@android:color/white"
                    android:textSize="35px" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20px"
                    android:text="籍贯："
                    android:textColor="@android:color/white"
                    android:textSize="35px" />

            </com.zhy.autolayout.AutoLinearLayout>


            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_id_no"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="right|center_vertical"
                    tools:text="44402382848754758"
                    android:textColor="@android:color/white"
                    android:textSize="35px" />

                <TextView
                    android:id="@+id/tv_birthday"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20px"
                    tools:text="1999.8.8"
                    android:textColor="@android:color/white"
                    android:textSize="35px" />

                <TextView
                    android:id="@+id/tv_nation"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20px"
                    tools:text="汉族"
                    android:textColor="@android:color/white"
                    android:textSize="35px" />

                <TextView
                    android:id="@+id/tv_origin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20px"
                    tools:text="厄瓜多尔"
                    android:textColor="@android:color/white"
                    android:textSize="35px" />
            </com.zhy.autolayout.AutoLinearLayout>

        </com.zhy.autolayout.AutoLinearLayout>
    </com.zhy.autolayout.AutoRelativeLayout>

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="1dp"
        android:background="@android:color/white"
        android:orientation="vertical">

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:background="@drawable/bg_stuinfo"
            android:orientation="vertical"
            android:padding="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="考生报考信息"
                android:textColor="@android:color/black"
                android:textSize="35px" />

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="250px"
                    android:layout_height="wrap_content"
                    android:text="持有证书："

                    android:textColor="@color/gray_a0"
                    android:textSize="35px" />

                <TextView
                    android:id="@+id/tv_old_certificate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:text="手风琴一级"


                    android:textColor="#333333"
                    android:textSize="35px" />
            </com.zhy.autolayout.AutoLinearLayout>

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="250px"
                    android:layout_height="wrap_content"
                    android:text="报考专业："

                    android:textColor="@color/gray_a0"
                    android:textSize="35px" />

                <TextView
                    android:id="@+id/tv_sign_major"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:text="手风琴"
                    android:textColor="#333333"
                    android:textSize="35px" />
            </com.zhy.autolayout.AutoLinearLayout>

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="250px"
                    android:layout_height="wrap_content"
                    android:text="报考等级："

                    android:textColor="@color/gray_a0"
                    android:textSize="35px" />

                <TextView
                    android:id="@+id/tv_sign_major_lvl"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:text="一级"
                    android:textColor="@color/colorPrimary"
                    android:textSize="35px" />
            </com.zhy.autolayout.AutoLinearLayout>

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="250px"
                    android:layout_height="wrap_content"
                    android:text="考试时间："

                    android:textColor="@color/gray_a0"
                    android:textSize="35px" />

                <TextView
                    android:id="@+id/tv_test_time"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:text="2017年9月28日 12:00- 1027年9余额29日 14:00"


                    android:textColor="#333333"
                    android:textSize="35px" />
            </com.zhy.autolayout.AutoLinearLayout>

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="250px"
                    android:layout_height="wrap_content"
                    android:text="考试地点："

                    android:textColor="@color/gray_a0"
                    android:textSize="35px" />

                <TextView
                    android:id="@+id/tv_site_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:text="神秘的美洲大陆神秘的美洲大陆神秘的美洲大陆神秘的美洲大陆"


                    android:textColor="#333333"
                    android:textSize="35px" />
            </com.zhy.autolayout.AutoLinearLayout>


            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="250px"
                    android:layout_height="wrap_content"
                    android:text="考室："

                    android:textColor="@color/gray_a0"
                    android:textSize="35px" />

                <TextView
                    android:id="@+id/tv_room_id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:text="考室（一）"


                    android:textColor="#333333"
                    android:textSize="35px" />
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="250px"
                    android:layout_height="wrap_content"
                    android:text="准考证号："

                    android:textColor="@color/gray_a0"
                    android:textSize="35px" />

                <TextView
                    android:id="@+id/tv_test_num"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:text="1999852577255544455"
                    android:textColor="#333333"
                    android:textSize="35px" />
            </com.zhy.autolayout.AutoLinearLayout>

        </com.zhy.autolayout.AutoLinearLayout>

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="1dp"
            android:gravity="bottom|center_horizontal"
            android:orientation="vertical">

            <Button
                android:id="@+id/bt_begin_assessment"
                android:layout_width="800px"
                android:layout_height="wrap_content"
                android:layout_marginBottom="80px"
                android:background="@drawable/button_base"
                android:text="@string/begin_assessment"
                android:textColor="@android:color/white"
                android:textSize="14sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="60px"
                android:text="@string/notice"
                android:textSize="30px" />
        </com.zhy.autolayout.AutoLinearLayout>

    </com.zhy.autolayout.AutoLinearLayout>
</com.zhy.autolayout.AutoLinearLayout>